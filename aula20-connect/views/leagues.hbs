{{#> layout}}
    <div class="panel panel-default">
        <table class="table">
            <tr>
                <th>Year</th>
                <th>League</th>
                <th>Caption</th>
            </tr>
            {{#each .}}
                <tr>
                    <td>{{year}}</td>
                    <td>{{league}}</td>
                    <td>{{> leagueLink}}</td>
                </tr>
            {{/each}}
        </table>
    </div>
{{/layout}}